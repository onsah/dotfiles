<application>
  <component name="CodeInsightSettings">
    <EXCLUDED_PACKAGE NAME="slick.model.ForeignKeyAction.Cascade$" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="0" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
  </component>
  <component name="InlayHintsSettings">
    <settingsMapElement>
      <kotlin.kotlin.references.types.hints>
        <Settings>
          <option name="localVariableType" value="true" />
        </Settings>
      </kotlin.kotlin.references.types.hints>
    </settingsMapElement>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="QQ:*">
      <option name="FOREGROUND" value="46c080" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="Hmm">
      <option name="FOREGROUND" value="c02cb4" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="5dab58" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="TEST:">
      <option name="FOREGROUND" value="7c033" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>